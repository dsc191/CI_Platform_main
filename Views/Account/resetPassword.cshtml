@{
    ViewData["Title"] = "Reset Password Page ";
}
    <div class="row main">

       <partial name="carousal"/>

        <div class="login-details1">
            <form>
                <h2 class="new-password" >New Password</h2>
                <p class="sub-text password-set" >Please enter a new password in the fields below.</p>
                <div class="mb-3">
                    <label class="form-label">New Password</label>
                    <input type="password" class="form-control InputField" aria-describedby="passwordHelp"
                        value="password">
                </div>
                <div class="mb-3">
                    <label class="form-label">confirm New Password</label>
                    <input type="password" class="form-control InputField" aria-describedby="passwordHelp"
                        value="password">
                </div>
                <button type="submit" class="btn btn-primary">Change Password</button>
                <div class="account">
                    <p class="sub-text"><a asp-area="" asp-controller="Account" asp-action="login" style="text-decoration:none">Login</a></p>

                </div>
                <div>
                    <p class="subline1 sub-text">Privacy Policy</p>
                </div>
            </form>
        </div>
    </div>

